<%= turbo_frame_tag dom_id(@post) do %>
<div class="card my-2" >
  <div class="card-body">
    <%= render partial: 'discussions/posts/header', locals: { post: @post } %>
    <%= render "discussions/posts/form", post: @post %>
    <%= link_to 'Back', discussion_post_path(@post.discussion, @post) %>
  </div>
</div>
<%end%>
